MASTER_CONTENT=$(cat /secure/cascade-oracle/*.py *.md *.txt *.json)
MASTER_HASH=$(echo "$MASTER_CONTENT" | sha512sum | awk '{print $1}')
echo "Master Content Hash: $MASTER_HASH"

